<!--pages/edit/edit.wxml-->
<view class="edit">
  <navigator url="../photo/photo" open-type="navigate">
    <view class="edit-info">
      <text>修改头像</text>
      <text class="iconfont icon-youjiantou-copy"></text>
    </view>
  </navigator>
    <navigator url="../name/name" open-type="navigate">
      <view class="edit-info">
        <text>修改昵称</text>
        <text class="iconfont icon-youjiantou-copy"></text>
      </view> 
    </navigator>
    <navigator url="../signature/signature" open-type="navigate">
    <view class="edit-info">
      <text>修改个性签名</text>
      <text class="iconfont icon-youjiantou-copy"></text>
    </view> 
    </navigator> 
    <navigator url="../location/location" open-type="navigate">
    <view class="edit-info">
      <text>共享位置</text>
      <text class="iconfont icon-youjiantou-copy"></text>
    </view>
    </navigator> 
    <navigator url="../phone/phone" open-type="navigate">
    <view class="edit-info">
      <text>设置手机号<text class="edit-tips">(仅好友可见)</text></text>
      <text class="iconfont icon-youjiantou-copy"></text>
    </view> 
  </navigator>
  <navigator url="../wx/wx" open-type="navigate">
    <view class="edit-info">
      <text>设置微信号<text class="edit-tips">(仅好友可见)</text></text>
      <text class="iconfont icon-youjiantou-copy"></text>
    </view> 
  </navigator>
</view>
